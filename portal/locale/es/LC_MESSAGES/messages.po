#: portal/templates/registered.html:10
#, python-format
msgid "You're now ready to access <em>%(hotspot_name)s's content</em>."
msgstr ""
"Ahora está listo para acceder al <em>contenido de %(hotspot_name)s</em>."

#: portal/templates/registered.html:11
#, python-format
msgid "Go to %(fqdn)s"
msgstr "Vaya a %(fqdn)s"
